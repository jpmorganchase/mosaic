    "d3": "^7.7.0",
    "deepmerge": "^2.0.1",
    "mermaid": "9.3.0",
    "mdx-mermaid": "^1.2.1",
    "warning": "^3.0.0"
  },
  "peerDependencies": {
    "@types/react": "^18.0.26",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
