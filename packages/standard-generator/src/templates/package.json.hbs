{
  "name": "{{ name }}",
  "version": "1.0.0",
  "license": "SEE LICENSE IN LICENSE.md",
  "private": true,
  "description": "{{ titleCase name }} Site generated by Mosaic generator",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "style": "dist/index.css",
  "scripts": {
    "build": "next build",
    "clean": "rm -fr public/.tmp .next",
    "dev":"next dev",
    "snapshot:latest": "yarn mosaic build --out public/snapshots --name latest --config mosaic.config.js",
    "start":"yarn site:serve:snapshot",
    "serve": "concurrently --kill-others 'yarn dev' 'yarn mosaic serve -c ./mosaic.config.js' -p 8080",
    "site:serve:snapshot": "yarn snapshot:latest && yarn build && cross-env MOSAIC_URL=http://localhost:3000/api/snapshots concurrently --kill-others 'next start'"
  },
  "dependencies": {
{{{ printDependencies dependencies }}}
    "@philpl/buble": "^0.19.7",
    "@types/react": "^16.9.0",
    "next": "^12.1.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "12.3.1",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "eslint-config-next": "^12.3.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  }
}
