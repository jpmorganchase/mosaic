    "bundle:components": "node  ../../scripts/bundle.js",
    "bundle:labs": "node ../../scripts/bundleLabs.js",
    "clean": "npx del-cli 'dist/**' 'dist-labs/**' && find . -type d -empty -delete",
    "lint": "eslint --ignore-pattern \"**/__tests__/**\"",
    "doc": "node ../../scripts/updateDocs.js",
    "watch": "node ../../scripts/bundle.js watch\" \"node ../../scripts/bundleLabs.js watch"
  },
