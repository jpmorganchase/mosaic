{
  "name": "@jpmorganchase/mosaic-site",
  "version": "0.1.0-beta.93",
  "license": "SEE LICENSE IN LICENSE.md",
  "private": true,
  "description": "@jpmorganchase/mosaic-site Site generated by Mosaic generator",
  "repository": {
    "type": "git",
    "url": "git@github.com:jpmorganchase/mosaic.git",
    "directory": "packages/site"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "style": "dist/index.css",
  "scripts": {
    "clean": "rm -fr public/.tmp .next",
    "build": "next build",
    "dev": "next dev",
    "start": "next start",
    "debug": "yarn mosaic serve -c ./mosaic.config.mjs -p 8080",
    "deploy": "yarn mosaic deploy --out snapshots --name latest --config mosaic.config.mjs",
    "docker:build": "docker build . --tag mosaic-site:latest",
    "docker:start": "docker run -it --rm --ipc=host -p 3000:3000 --network mosaic-net --name mosaic-site mosaic-site:latest",
    "e2e": "yarn playwright test",
    "e2e:codegen": "yarn playwright codegen localhost:3000",
    "gen:snapshot": "yarn mosaic build --out snapshots --name latest --config mosaic.config.mjs",
    "serve:snapshot:file": "yarn cross-env MOSAIC_MODE=\"snapshot-file\" concurrently --kill-others \"yarn dev\"",
    "serve:snapshot:s3": "yarn cross-env MOSAIC_MODE=\"snapshot-s3\" concurrently --kill-others \"yarn dev\"",
    "serve": "concurrently --kill-others \"yarn dev\" \"yarn mosaic serve -c ./mosaic.config.mjs\" -p 8080"
  },
  "dependencies": {
    "@jpmorganchase/mosaic-cli": "^0.1.0-beta.93",
    "@jpmorganchase/mosaic-components": "^0.1.0-beta.93",
    "@jpmorganchase/mosaic-content-editor-plugin": "^0.1.0-beta.93",
    "@jpmorganchase/mosaic-layouts": "^0.1.0-beta.93",
    "@jpmorganchase/mosaic-site-components": "^0.1.0-beta.93",
    "@jpmorganchase/mosaic-site-preset-styles": "^0.1.0-beta.93",
    "@jpmorganchase/mosaic-sitemap-component": "^0.1.0-beta.93",
    "@jpmorganchase/mosaic-source-git-repo": "^0.1.0-beta.93",
    "@jpmorganchase/mosaic-source-local-folder": "^0.1.0-beta.93",
    "@jpmorganchase/mosaic-standard-generator": "^0.1.0-beta.93",
    "@jpmorganchase/mosaic-store": "^0.1.0-beta.93",
    "@jpmorganchase/mosaic-theme": "^0.1.0-beta.93",
    "@salt-ds/core": "^1.45.0",
    "@salt-ds/lab": "1.0.0-alpha.67",
    "@types/react": "^18.3.12",
    "https-proxy-agent": "^7.0.0",
    "next": "^14.2.32",
    "next-auth": "^4.24.5"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.0.0",
    "@playwright/test": "^1.49.1",
    "@types/node": "^20.0.0",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "dotenv-load": "^2.0.1",
    "eslint-config-next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
