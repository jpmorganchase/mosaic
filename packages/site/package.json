{
  "name": "@jpmorganchase/mosaic-site",
  "version": "1.0.0",
  "license": "SEE LICENSE IN LICENSE.md",
  "private": true,
  "description": "@Jpmorganchase/mosaic-Site Site generated by Mosaic generator",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "style": "dist/index.css",
  "scripts": {
    "mosaic": "node ../cli/dist/index.js",
    "build": "next build",
    "clean": "rm -fr public/.tmp .next",
    "dev":"next dev",
    "gen:snapshot": "yarn mosaic build --out public/snapshots --name latest --config mosaic.config.js",
    "serve:snapshot": "yarn cross-env MOSAIC_URL=http://localhost:3000/api/snapshots concurrently --kill-others 'yarn dev'",
    "serve": "concurrently --kill-others 'yarn dev' 'yarn mosaic serve -c ./mosaic.config.js' -p 8080"
  },
  "dependencies": {
    "@jpmorganchase/mosaic-components": "^0.1.0-beta.9",
    "@jpmorganchase/mosaic-content-editor-plugin": "^0.1.0-beta.9",
    "@jpmorganchase/mosaic-layouts": "^0.1.0-beta.9",
    "@jpmorganchase/mosaic-site-components": "^0.1.0-beta.9",
    "@jpmorganchase/mosaic-site-preset-styles": "^0.1.0-beta.9",
    "@jpmorganchase/mosaic-store": "^0.1.0-beta.9",
    "@jpmorganchase/mosaic-theme": "^0.1.0-beta.9",
    "@jpmorganchase/mosaic-cli": "^0.1.0-beta.9",
    "@jpmorganchase/mosaic-core": "^0.1.0-beta.9",
    "@jpmorganchase/mosaic-plugins": "^0.1.0-beta.9",
    "@jpmorganchase/mosaic-schemas": "^0.1.0-beta.9",
    "@jpmorganchase/mosaic-serialisers": "^0.1.0-beta.9",
    "@jpmorganchase/mosaic-source-git-repo": "^0.1.0-beta.9",
    "@jpmorganchase/mosaic-source-local-folder": "^0.1.0-beta.9",
    "@jpmorganchase/mosaic-types": "^0.1.0-beta.9",
    "@philpl/buble": "^0.19.7",
    "@types/react": "^16.9.0",
    "next": "^12.1.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "12.3.1",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "eslint-config-next": "^12.3.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  }
}
