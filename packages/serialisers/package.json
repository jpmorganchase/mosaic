{
  "name": "@jpmorganchase/mosaic-serialisers",
  "version": "0.1.0-beta.39",
  "author": "",
  "description": "Mosaic serializers for core file system",
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    "./mdx": {
      "types": "./dist/mdx.d.ts",
      "import": "./dist/mdx.js",
      "node": "./dist/mdx.js"
    },
    "./json": {
      "types": "./dist/json.d.ts",
      "import": "./dist/json.js",
      "node": "./dist/json.js"
    },
    "./md": {
      "types": "./dist/md.d.ts",
      "import": "./dist/md.js",
      "node": "./dist/md.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "node": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=16.10.0 || >=18.0.0"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -fr dist",
    "lint": "eslint --ignore-pattern \"**/__tests__/**\""
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jpmorganchase/mosaic.git",
    "directory": "packages/serialisers"
  },
  "devDependencies": {
    "rxjs": "^7.5.5"
  },
  "dependencies": {
    "@jpmorganchase/mosaic-types": "^0.1.0-beta.39",
    "gray-matter": "^4.0.3"
  }
}
