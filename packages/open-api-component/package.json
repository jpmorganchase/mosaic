    "directory": "packages/open-api-component"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "style": "./dist/index.css",
  "exports": {
    "./index.css": "./dist/index.css",
    ".": {
      "style": "./dist/index.css",
