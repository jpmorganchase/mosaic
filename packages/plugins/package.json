{
  "name": "@jpmorganchase/mosaic-plugins",
  "version": "0.1.0-beta.9",
  "author": "",
  "description": "Mosaic plugins for core file system",
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "engines": {
    "node": ">= 12.0.0"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && tsc && swc ./src/*.mjs.ts -d dist && renamer --find \".mjs.js\" --replace \".mjs\" \"dist/*.mjs.js\" && renamer --find \".mjs.js.map\" --replace \".mjs.map\" \"dist/*.mjs.js.map\"",
    "clean": "rm -fr dist",
    "lint": "eslint --ignore-pattern \"**/__tests__/**\""
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jpmorganchase/mosaic.git"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.237",
    "@types/fs-extra": "^9.0.13",
    "renamer": "^4.0.0"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^9.0.9",
    "@jpmorganchase/mosaic-serialisers": "^0.1.0-beta.9",
    "@jpmorganchase/mosaic-source-git-repo": "^0.1.0-beta.9",
    "@jpmorganchase/mosaic-source-local-folder": "^0.1.0-beta.9",
    "@jpmorganchase/mosaic-types": "^0.1.0-beta.9",
    "@types/github-slugger": "^1.3.0",
    "@vcarl/remark-headings": "^0.1.0",
    "fs-extra": "^10.1.0",
    "github-slugger": "^1.4.0",
    "lodash": "^4.17.21",
    "memfs": "^3.3.0",
    "reading-time": "^1.5.0",
    "remark-mdx": "^2.1.5",
    "remark-parse": "^10.0.1",
    "remark-stringify": "^10.0.2",
    "rxjs": "^7.5.5",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.1",
    "uuid": "^7.0.3"
  }
}
